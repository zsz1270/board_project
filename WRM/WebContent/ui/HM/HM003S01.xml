	};
	
	scwin.searchZipCodeCallback = function(returnValue) {
		var selectedZipCode = returnValue;
		adm_memberBasic.set("ZIP_CD", selectedZipCode.ZIP_CD);
		adm_memberBasic.set("ADDRESS1", selectedZipCode.ADDRESS);
		ibx_address2.focus();
	};
	
	]]></script>
