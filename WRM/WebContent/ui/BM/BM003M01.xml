					dataList="data:dlt_program" defaultCellHeight="37" id="grd_program"
					rowStatusWidth="50" scrollByColumn="false" scrollByColumnAdaptive="false" style="height:340px;" visibleRowNum="7"
					rowStatusVisible="true" rowStatusHeaderValue="상태" keepDefaultColumnWidth="true" ev:onrowindexchange="scwin.grd_program_onrowindexchange" sortable="">
