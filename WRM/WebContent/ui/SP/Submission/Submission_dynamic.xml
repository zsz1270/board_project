		<script type="text/javascript" lazy="false"><![CDATA[ 
	//화면 초기 로딩 시 호출 - config.xml 설정에 화면 로딩 후 자동 호출된다.
	scwin.onpageload = function() {
		com.win.setEnterKeyEvent(grp_search, scwin.btn_search_onclick);
	};
	
	scwin.btn_search_onclick = function() {
		if (com.data.validateGroup(grp_search) === false) {
			return;
		} else {
			var searchCodeGrpOption = { id : "sbm_SelectTempDynamic", 
										action : "/sample/tempSelect", ref : "data:json,dma_search",
										target : 'data:json,{"id":"dlt_temp","key":"dlt_temp"}', 
										submitDoneHandler : scwin.sbm_SelectTemp_submitdone, isProcessMsg : false };
			com.sbm.executeDynamic(searchCodeGrpOption);
		}
	};
	
	scwin.sbm_SelectTemp_submitdone = function(e) {
		if (com.sbm.getResultCode(e) == gcm.MESSAGE_CODE.STATUS_SUCCESS) {
			com.win.alert("Submission 호출에 성공했습니다.")
		}
	};
]]></script>
